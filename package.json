{
    "name": "fitracker",
    "version": "2.10.0",
    "main": "expo-router/entry",
    "scripts": {
        "start": "cross-env APP_VARIANT=development expo start --dev-client",
        "start:cache": "cross-env APP_VARIANT=development expo start --dev-client -c --clear",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "lint": "eslint . --ext .ts --ext .tsx",
        "lint:fix": "eslint . --ext .ts --ext .tsx --fix",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc",
        "eas:update": "cross-env APP_VARIANT=production eas update --channel %npm_config_channel% --message \"%npm_config_message%\"",
        "eas:update:preview": "cross-env APP_VARIANT=test eas update --channel preview --message \"%npm_config_message%\"",
        "eas:build:dev": "eas build --profile development --platform ios --clear-cache",
        "version:patch": "npm version --no-git-tag-version patch",
        "version:minor": "npm version --no-git-tag-version minor",
        "version:major": "npm version --no-git-tag-version major",
        "api:update": "npx openapi-generator-cli generate -i http://localhost:3000/api-json  -g typescript-axios  -o ./src/api/generated"
    },
    "dependencies": {
        "@expo/metro-runtime": "^3.2.1",
        "@gorhom/bottom-sheet": "^4.6.4",
        "@gorhom/portal": "^1.0.14",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@reduxjs/toolkit": "^2.2.7",
        "@tamagui/babel-plugin": "^1.110.2",
        "@tamagui/config": "^1.110.2",
        "@tanstack/react-query": "^5.40.0",
        "@types/react": "~18.2.79",
        "@types/styled-components": "^5.1.26",
        "axios": "^1.7.2",
        "babel-plugin-module-resolver": "^5.0.2",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "expo": "^51.0.39",
        "expo-blur": "~13.0.2",
        "expo-constants": "~16.0.2",
        "expo-dev-client": "^4.0.29",
        "expo-device": "~6.0.2",
        "expo-font": "~12.0.10",
        "expo-linking": "~6.3.1",
        "expo-router": "^3.5.24",
        "expo-secure-store": "^13.0.2",
        "expo-status-bar": "~1.12.1",
        "expo-updates": "^0.25.27",
        "lodash": "^4.17.21",
        "react": "18.2.0",
        "react-content-loader": "^7.0.2",
        "react-native": "0.74.5",
        "react-native-animateable-text": "^0.12.0",
        "react-native-dotenv": "^3.4.11",
        "react-native-draggable-flatlist": "^4.0.1",
        "react-native-element-dropdown": "^2.12.4",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-progress": "^5.0.1",
        "react-native-reanimated": "~3.10.1",
        "react-native-safe-area-context": "4.10.5",
        "react-native-screens": "3.31.1",
        "react-native-svg": "15.2.0",
        "react-redux": "^9.1.2",
        "tamagui": "^1.110.2",
        "typescript": "^4.9.4",
        "uuid": "^11.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@types/lodash": "^4.17.7",
        "@types/react-native-get-random-values": "^1.8.0",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^36.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "^3.0.0",
        "react-native-get-random-values": "~1.11.0",
        "typescript": "~5.3.3"
    },
    "private": true
}
